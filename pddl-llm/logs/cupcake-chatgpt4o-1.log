```domain.pddl
(define (domain cupcake-world)
  (:requirements :strips :typing)
  (:types 
    gripper 
    item)

  (:predicates 
    (gripper-empty ?g - gripper)
    (holding ?g - gripper ?i - item)
    (on-table ?i - item)
    (on-plate ?i - item))

  (:action pickup
    :parameters (?g - gripper ?i - item)
    :precondition (and (gripper-empty ?g) (on-table ?i))
    :effect (and (holding ?g ?i) (not (gripper-empty ?g)) (not (on-table ?i))))

  (:action place-on-plate
    :parameters (?g - gripper ?i - item)
    :precondition (holding ?g ?i)
    :effect (and (on-plate ?i) (gripper-empty ?g) (not (holding ?g ?i))))
)
```

```problem.pddl
(define (problem cupcake-task)
  (:domain cupcake-world)
  (:objects
    grippy - gripper
    cupcake - item)

  (:init
    (gripper-empty grippy)
    (on-table cupcake))

  (:goal
    (and (on-plate cupcake) (gripper-empty grippy))))

